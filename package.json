{
  "name": "hafiportrait-web-dashboard",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev -H 0.0.0.0 -p ${PORT:-3002}",
    "dev:docker": "next dev -H 0.0.0.0 -p 3002",
    "dev:public": "concurrently \"NEXT_PUBLIC_SOCKETIO_URL=http://147.251.255.227:3003 pnpm run dev:public\" \"pnpm run socketio:public:dev\"",
    "build": "next build",
    "build:staging": "next build",
    "start": "next start -p ${PORT:-3000}",
    "lint": "next lint",
    "env:dev": "echo 'Manual env mode - edit .env.local directly'",
    "env:prod": "echo 'Manual env mode - edit .env.production directly'",
    "env:staging": "echo 'Manual env mode - edit .env.staging directly'",
    "env:status": "node scripts/development/env-status.js",
    "ws:start": "node scripts/utils/start-websocket-server.js",
    "ws:dev": "nodemon scripts/utils/start-websocket-server.js",
    "socketio:start": "node scripts/utils/start-socketio-server.js",
    "socketio:dev": "nodemon scripts/utils/start-socketio-server.js",
    "socketio:local": "node socketio-server.js",
    "socketio:local:dev": "nodemon socketio-server.js",
    "socketio:public": "SOCKETIO_PORT=3003 node socketio-server.js",
    "socketio:public:dev": "SOCKETIO_PORT=3003 nodemon socketio-server.js",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:start:prod": "pm2 start ecosystem.production.config.js",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:logs": "pm2 logs",
    "pm2:status": "pm2 status",
    "dev:full": "concurrently \"pnpm run dev\" \"pnpm run ws:start\"",
    "dev:socketio": "concurrently \"pnpm run dev\" \"pnpm run socketio:start\"",
    "test:socketio": "NEXT_PUBLIC_USE_SOCKETIO=true pnpm run dev:socketio",
    "test:websocket": "NEXT_PUBLIC_USE_SOCKETIO=false pnpm run dev:full",
    "cloudrun:build": "docker build -t hafiportrait .",
    "cloudrun:run": "docker run -p 8080:8080 hafiportrait",
    "install:all": "pnpm install",
    "clean": "rm -rf node_modules pnpm-lock.yaml",
    "fresh-install": "pnpm run clean && pnpm run install:all",
    "monitor:start": "node scripts/monitoring/enhanced-health-monitor.js start",
    "monitor:check": "node scripts/monitoring/enhanced-health-monitor.js check",
    "monitor:status": "node scripts/monitoring/enhanced-health-monitor.js status",
    "monitor:system": "bash scripts/monitoring/setup-monitoring.sh",
    "alerts:send": "node scripts/monitoring/alert-manager.js send",
    "alerts:history": "node scripts/monitoring/alert-manager.js history",
    "alerts:stats": "node scripts/monitoring/alert-manager.js stats",
    "deploy:railway": "railway up",
    "deploy:render": "echo 'Setup manual di Render dashboard'",
    "deploy:netlify": "netlify deploy --prod",
    "test:db": "node -e \"console.log('Testing database connection...'); require('./src/lib/database.ts').testConnection()\"",
    "test:r2": "node -e \"console.log('Testing R2 storage...'); console.log('Use admin dashboard for R2 testing')\"",
    "test:all": "pnpm run test:db && pnpm run test:r2"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.864.0",
    "@aws-sdk/s3-request-presigner": "^3.864.0",
    "@radix-ui/react-accordion": "^1.2.12",
